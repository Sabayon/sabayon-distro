diff -Nurp pulseaudio-0.9.15.orig/src/pulsecore/database-gdbm.c pulseaudio-0.9.15/src/pulsecore/database-gdbm.c
--- pulseaudio-0.9.15.orig/src/pulsecore/database-gdbm.c	1970-01-01 01:00:00.000000000 +0100
+++ pulseaudio-0.9.15/src/pulsecore/database-gdbm.c	2009-06-19 10:09:35.620627215 +0200
@@ -0,0 +1,249 @@
+/***
+  This file is part of PulseAudio.
+
+  Copyright 2009 Lennart Poettering
+
