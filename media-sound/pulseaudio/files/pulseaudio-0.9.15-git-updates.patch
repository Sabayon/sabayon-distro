+#define SELEM_INIT(sid, name)                           \
+    do {                                                \
+        snd_mixer_selem_id_alloca(&(sid));              \
+        snd_mixer_selem_id_set_name((sid), (name));     \
+        snd_mixer_selem_id_set_index((sid), 0);         \
+    } while(FALSE)
