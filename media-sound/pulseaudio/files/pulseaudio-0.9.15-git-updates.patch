diff -Nurp pulseaudio-0.9.15.orig/src/modules/module-udev-detect.c pulseaudio-0.9.15/src/modules/module-udev-detect.c
--- pulseaudio-0.9.15.orig/src/modules/module-udev-detect.c	1970-01-01 01:00:00.000000000 +0100
+++ pulseaudio-0.9.15/src/modules/module-udev-detect.c	2009-06-19 10:09:35.599620243 +0200
@@ -0,0 +1,457 @@
+/***
